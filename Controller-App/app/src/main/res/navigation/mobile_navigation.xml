<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_target">

    <fragment
        android:id="@+id/navigation_target"
        android:name="io.github.igneel32.remote.ui.target.TargetFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_target" />

    <fragment
        android:id="@+id/navigation_matchplay"
        android:name="io.github.igneel32.remote.ui.matchplay.MatchplayFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_matchplay" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="io.github.igneel32.remote.ui.settings.SettingsFragment"
        android:label="@string/settings"
        tools:layout="@layout/fragment_settings" />
</navigation>